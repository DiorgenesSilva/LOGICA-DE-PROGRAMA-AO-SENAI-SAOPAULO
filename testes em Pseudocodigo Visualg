Algoritmo "Quantidade de DEpendentes de um funcionario  "

Var
   nome:caractere
   SAL,NSAL: real
   DEP:inteiro

Inicio
      escreva (" qual o nome do funcionario")
      leia(nome)
      escreval ("Qual o salario do funcionario ")
      leia(SAL)
      escreval ("Qual a quantidade de dependentes que ele possui ?")
      leia( DEP )
      escolha(DEP)
      caso   0
      NSAL <- SAL + ( SAL * 5/100 )
      caso 1,2,3
      NSAL <- SAL + ( SAL * 10/100 )
      caso 4,5,6
      NSAL <- SAL + ( SAL * 15/100 )
      outrocaso
      NSAL <- SAL + (SAL * 18/100 )
      
   fimescolha
   escreval("O novo salario de" , nome , "sera de R$ ", NSAL :5:2 )
      
Fimalgoritmo

