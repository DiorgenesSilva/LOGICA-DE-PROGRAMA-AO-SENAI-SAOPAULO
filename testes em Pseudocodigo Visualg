Algoritmo "C EStrutura Condicional aninhada,media do aluno"

var
   N1,N2,M: real

Inicio
   escreva ("Primeira nota")
   leia ( N1 )
   escreva ("Segunda Nota :")
   leia ( N2 )
   M <- (N1 + N2)/2
   escreva ( "A media final  do aluno foi ",M:4:2)
   se ( M >= 7 ) entao
      escreval ("Aluno esta APROVADO ")
                senao
                se (M >= 5 ) e ( M < 7 )entao
                escreval ("Aluno RECUPERAÇÃO ")
                senao
                escreval ( " Aluno REPROVADO")
   fimse

Fimalgoritmo
--------------------------------------------------------------------------------
----------------------------------------------------------------------------------------
Algoritmo "Calcule IMC Massa Corporal e altua em Metro s"

var
   massa:real
   altura:real
   imc:real

Inicio
      escreva ("Massa Kg:")
      leia ( massa )
      escreva ( "Altura Mts:")
      leia ( altura )
      imc <- massa /( altura ^2)
      escreval("IMC=",imc:5:1)
      se ( imc < 17)entao
      escreval ("Karalho !! TA PASSANDO FOME  ")
        senao
        se (imc  >= 17 ) e ( imc < 18.5 )entao
        escreva ("vAI COMER FILHA DA PUTA TA SÓ O PÓ ")
          senao
          se (imc >= 18.5) e ( imc < 25 ) entao
          escreva ("TA NO PONTO HEIN SE DER EU COMO !!")
            senao
            se (imc >= 25 ) e (imc < 30 )entao
            escreva ("TA AIMA DO PESO ,VAI FAZER ALGO PRA MUDAR ISSO MERDA!!")
              senao (imc >= 30) e  ( imc < 35 ) entao
              escreva ("OHH PORPETA VC NAO,VAI FAZER DIETA OU CORRER NAO??")
                senao
                 se (imc >= 35 ) e ( imc < 40 ) entao
                 escreva ("CHUPETA DE BALEIA DO KARALHO,GORDONZILLA VAI EMAGRECER ")
                  senao
                  se (imc > 40)
                  escreva ("NAO CONSEGUE NEM ADAR NÉ O GORDAO BALOFO,QUER UM EMPURRAO PRA DESCER ROLANDO")
                fimse
               fimse
              fimse
            fimse
            fimse
           fimse
          fimse


    Fimalgoritmo

