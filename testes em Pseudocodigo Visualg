    Algoritmo "Criança Esperança estrutura faça-caso "

Var
  d,valor:inteiro

Inicio
       escreval("-------------------------------------------------")
       escreval("~~~~~~~~~~~CRIANÇA ESPERANÇA~~~~~~~~~~~~~~~~~~")
       escreval ("------------------------------------------------")
       escreval("S2S2S2S2****MUITO OBRIGADO POR AJUDAR *****S2S2S2S2")
       escreval("Aperte 1 para doar R$10,00")
       escreval("Aperte 2 para doar R$25,00")
       escreval("Aperte 3 para doar R$50,00 ")
       escreval("Aperte 4 para digitar qualquer valor ")
       escreval("Aperet 5 para Cancelar  ")
       leia(d)
       escolha(d)
           caso 1
           valor <- 10
           caso 2
           valor <- 25
           caso 3
           valor  <- 50
           caso 4
           escreva ("Qual o valor que voce deseja doar ? R$:")
           leia(valor)
           caso 5
           valor <- 0
           fimescolha
      escreval( "Sua doação foi de R$:",valor , "MUITO OBRIGADO !!!")
      escreval ("~~~~~~~~~~~~~***CRIANÇA ESPERANÇA AGRADECE***~~~~~~~~~~~~~~~~")
      

Fimalgoritmo

