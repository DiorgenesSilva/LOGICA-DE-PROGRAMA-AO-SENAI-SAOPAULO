Algoritmo " Contar ate onde o usuario quiser  ESTRUTURA DE REPETIÇÃO enquanto-faça   "

Var

   CONTADOR: inteiro
   VALOR: inteiro
   

Inicio
     CONTADOR <- 0
     escreval("Quer contar ate quanto ?")
     leia(VALOR)
     enquanto ( CONTADOR <= VALOR ) faça
     escreval(CONTADOR)
     CONTADOR <- CONTADOR  + 1
     
     fimenquanto
       escreval("TERMINEI  DE CONTAR ")



Fimalgoritmo

------------------------------------------------------------------------------------------------------------------
-------------------------------------------------------------------------------------------------------------------

Algoritmo " Contar ate onde o usuario quiser,UTILIZANDO SALTO   ESTRUTURA DE REPETIÇÃO enquanto-faça   "

Var

   CONTADOR: inteiro
   VALOR: inteiro
   SALTO: inteiro


Inicio
     CONTADOR <- 0
     escreval ("qUAL SERA O VALOR DO SALTO ")
     leia(SALTO)
     escreval("Quer contar ate quanto ?")
     leia(VALOR)
     enquanto ( CONTADOR <= VALOR ) faça
     escreval(CONTADOR)
     CONTADOR <- CONTADOR  + SALTO

     fimenquanto
       escreval("TERMINEI  DE CONTAR ")



Fimalgoritmo

