---------------------------------------------------------------------------------------------------------------

Algoritmo " Fazer Conversão de Moedas 4X  "

vAR
   D,R,C : real

   
Inicio
    C <- 1
    enquanto ( C <= 4 ) faça
          escreval ("Qual o valor em R$ ")
          leia ( R )
          D <- R / 4.10
          escreval(" O valor convertido em US$",D:5:2)
          C <- C +1
          
          
          
   fimenquanto
    

Fimalgoritmo

